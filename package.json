{
  "name": "yawik-nuxt",
  "version": "1.0.0",
  "description": "YAWIK Frontend based on nuxt",
  "author": "Carsten Bleek",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
